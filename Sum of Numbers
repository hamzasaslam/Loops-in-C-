//This Program shows the Execution of Loops
//Print Numbers 1-10
//Sum of first 10 natural numbers
#include <iostream>
using namespace std;
int main(){
    int a, sum=0;
    cout<<"The first 10 natural numbers are: ";
    cout<<"********************************* ";
    for(a=1; a<=10; a++){
        cout<<a<<endl;
        sum=sum+a;
    }
    cout<<"The Sum of first 10 natural number= "<<sum;
    return 0;
}
